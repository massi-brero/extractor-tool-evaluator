<?xml version="1.0" encoding="UTF-8"?>
   <!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="de.mbrero.see.persistance.dto.TestRun" table="test_run">
		<id name="id">
			<generator class="increment" />
		</id>
		<property name="inputPath" />
		<property name="outputPathExtractorResult" />
		<property name="outputPathTRECFile" />
		<property name="date" />
		<property name="result" column="result">
		    <type name="org.hibernate.type.EnumType.String">
		        <param name="enumClass">de.mbrero.see.persistance.dto.types.TestRunResults.java</param>
                <param name="useNamed">false</param>
		    </type>
		</property>
		<property name="parameters" />
		<property name="duration" />
		<property name="tester" />
		<property name="systemInformation" />
	</class>
</hibernate-mapping>  